<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="515.0" prefWidth="697.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CalendarController">
   <children>

      <TableView fx:id="tvCalendar" layoutX="8.0" layoutY="32.0" prefHeight="475.0" prefWidth="592.0" />

      <Label fx:id="lbltitle" layoutX="264.0" layoutY="10.0" text="CALENDAR HOLYDAYS" />
      <Button fx:id="btnGuardar" layoutX="612.0" layoutY="81.0" mnemonicParsing="false" onMouseClicked="#saveBtnEvent" text="GUARDAR" />
      <Button fx:id="btnEditar" layoutX="611.0" layoutY="32.0" mnemonicParsing="false" onMouseClicked="#editBtnEvent" text="EDITAR" />
      <Button fx:id="btnCancelar" layoutX="612.0" layoutY="134.0" mnemonicParsing="false" onMouseClicked="#cancelBtnEvent" text="CANCELAR" />
      <Button fx:id="btnAtras" layoutX="624.0" layoutY="450.0" mnemonicParsing="false" onMouseClicked="#backBtnEvent" text="ATRAS" />
   </children>
</AnchorPane>
