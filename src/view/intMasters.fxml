<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MastersList">
    <children>
        <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <fx:include source="intHeader.fxml" />
                <HBox alignment="CENTER" maxWidth="960.0" VBox.vgrow="ALWAYS">
                    <children>
                        <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" style="-fx-background-color: transparent;" HBox.hgrow="ALWAYS">
                            <content>
                                <AnchorPane maxHeight="-Infinity" maxWidth="760.0" minHeight="-Infinity" nodeOrientation="LEFT_TO_RIGHT">
                                    <children>
                                        <HBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <children>
                                                <VBox fx:id="master_col_1" alignment="CENTER_RIGHT" spacing="20.0">
                                                    <HBox.margin>
                                                        <Insets right="10.0" />
                                                    </HBox.margin>
                                                </VBox>
                                                <VBox fx:id="master_col_2" alignment="CENTER_LEFT" spacing="20.0">
                                                    <HBox.margin>
                                                        <Insets left="10.0" />
                                                    </HBox.margin>
                                                </VBox>
                                            </children>
                                        </HBox>
                                    </children>
                                </AnchorPane>
                            </content>
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </ScrollPane>
                        <VBox alignment="TOP_RIGHT" minWidth="200.0" spacing="20.0">
                            <children>
                                <Button fx:id="master_agregar" ellipsisString="" mnemonicParsing="false" onAction="#newMaster" style="-fx-text-fill: #000000; -fx-background-color: #dddddd; -fx-border-color: #dddddd; -fx-border-radius: 4px; -fx-background-radius: 4px;" text="Crear nuevo" textOverrun="CLIP" wrapText="true" VBox.vgrow="ALWAYS">
                                    <font>
                                        <Font size="16.0" />
                                    </font>
                                    <VBox.margin>
                                        <Insets />
                                    </VBox.margin>
                                </Button>
                            </children>
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </VBox>
                    </children>
                    <VBox.margin>
                        <Insets bottom="20.0" top="20.0" />
                    </VBox.margin>
                </HBox>
                <fx:include source="intFooter.fxml" />
            </children>
        </VBox>
    </children>
</AnchorPane>
